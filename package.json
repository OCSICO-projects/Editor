  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "start-aot": "ng serve --aot --open",
    "start-hmr": "ng serve --hmr -e=hmr -sm=false",
    "start-hmr-sourcemaps": "ng serve --hmr -e=hmr",
    "build": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --dev",
    "build-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --dev --stats-json",
    "build-stage": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --env=stage",
    "build-prod": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod",
    "build-prod-optimizer": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod --build-optimizer",
    "build-prod-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "build-player": "node ./player/rollup.js"
  },
